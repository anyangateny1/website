            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "lambda:AddPermission",
                "lambda:GetFunction",
                "lambda:RemovePermission"
            ],
            "Resource": "arn:aws:lambda:ap-southeast-2:739275456268:function:personal-website-backend"
        }
    ]
} 